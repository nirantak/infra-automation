{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "padding": {
      "top": 1,
      "left": 1
    }
  },
  "modules": [
    "title",
    "datetime",
    "break",
    "os",
    "host",
    "bios",
    "bootmgr",
    "board",
    "chassis",
    "kernel",
    "display",
    "break",
    "users",
    "uptime",
    "loadavg",
    "processes",
    {
      "type": "cpu",
      "showPeCoreCount": true,
      "temp": true
    },
    "cpuusage",
    {
      "type": "gpu",
      "driverSpecific": true,
      "temp": true
    },
    "memory",
    "physicalmemory",
    "swap",
    "disk",
    "break",
    "wifi",
    "dns",
    "publicip",
    {
      "type": "localip",
      "showAllIps": true,
      "showMac": true
    },
    "break"
  ]
}
